{
  "name": "danoc.me",
  "private": true,
  "scripts": {
    "start": "gatsby develop",
    "test": "yarn test:eslint && yarn test:prettier",
    "test:eslint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore",
    "test:prettier": "prettier \"**/*.{js,jsx,css,scss,json,md,mdx,html}\" --list-different",
    "format": "prettier \"**/*.{js,jsx,css,scss,json,md,mdx,html}\" --write",
    "build": "TZ=America/Los_Angeles && gatsby build"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@emotion/core": "^10.0.4",
    "@emotion/styled": "^10.0.12",
    "dotenv": "^8.2.0",
    "emotion": "^10.0.4",
    "emotion-server": "^10.0.4",
    "gatsby": "^2.9.2",
    "gatsby-plugin-emotion": "^4.0.7",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-feed": "^2.2.3",
    "gatsby-plugin-google-analytics": "^2.0.21",
    "gatsby-plugin-lodash": "^3.0.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-offline": "^3.0.24",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sharp": "^2.1.5",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.0.14",
    "gatsby-remark-prismjs": "^3.2.11",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.39",
    "gatsby-source-pinboard": "^0.1.6",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.21",
    "hex-rgba": "^1.0.2",
    "is-relative-url": "^3.0.0",
    "lodash": "^4.17.5",
    "node-sass": "^4.12.0",
    "prismjs": "^1.14.0",
    "prop-types": "^15.6.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^3.1.0",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
